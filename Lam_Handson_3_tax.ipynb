    "taxRate = 0.06\n",
    "\n",
    "def salesTax(num):\n",
    "    return round(num*taxRate,2)\n",
    "\n",
    "def afterTax(num):\n",
    "    return round(num+salesTax(num),2)\n",
    "\n",
    "def main():\n",
    "    print(str(salesTax(100)))\n",
    "    print(str(afterTax(100)))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()"
